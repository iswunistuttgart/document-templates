      x=1.00cm,%
      y=-1.00cm,%
    ]
% Draw row names i.e., color names
    \foreach \color [count=\y from 0] in \ustuttcolorlist {
      \draw[%
        ]%
        (0,\y)%
          node[%
              align=right,%
              anchor=east,%
            ]%
            {\color};
    }
% Draw column names i.e., hue names
    \foreach \hue [count=\x from 0] in \ustutthuelist {
      \draw[
        ]%
        (\x+0.50,-0.50)%
          node[%
              anchor=west,%
              align=left,%
              rotate=90,%
            ]%
            {\hue};
    }
% Draw the rectangles
    \foreach \color [count=\y from 0] in \ustuttcolorlist {
      \draw[%
        ]%
        (0,\y)%
          node[%
              align=right,%
              anchor=east,%
            ]%
            {\color};
      \foreach \hue [count=\x from 0] in \ustutthuelist {
        \draw[
            color=\color\hue,%
            draw,%
            fill,%
            fill opacity=1.00,%
            draw opacity=1.00,%
          ]%
          (\x,\y-0.50)%
            rectangle ++(1.00,1.00);
      }
    }
  \end{tikzpicture}
}
